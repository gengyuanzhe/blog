<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN" monitorInterval="6">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss.SSS} UTC][%-5p][%F:%M,%L][%t] %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</configuration>